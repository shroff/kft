$ ./RPNcl-udp 127.0.0.1 13000 "-2 3 - 4 *"
-20

$ ./RPNcl-udp 127.0.0.1 13000 "-2 3 + 4 * 2 /"
2

$ ./RPNcl-udp 127.0.0.1 13000 "-2 3 + 4 * 2 %"
0

$ ./RPNcl-udp 127.0.0.1 13000 "-2 3 + 4 * 2 a"
invalid


$ ./RPNcl-tcp 127.0.0.1 13000 "-2 3 - 4 *"
-20

$ ./RPNcl-tcp 127.0.0.1 13000 "-2 3 + 4 * 2 /"
2

$ ./RPNcl-tcp 127.0.0.1 13000 "-2 3 + 4 * 2 %"
0

$ ./RPNcl-tcp 127.0.0.1 13000 "-2 3 + 4 * 2 a"
invalid
